  "dependencies": [
    {"name":"puppetlabs-stdlib","version_requirement":">= 4.24.0 <5.0.0"},
    {"name":"icann-quagga","version_requirement":">= 0.4.3 <0.5.0"},
    {"name":"icann-openbgpd","version_requirement":">= 0.2.1 <0.3.0"},
    {"name":"icann-tea","version_requirement":">= 0.2.2 <0.3.0"},
    {"name":"icann-motd","version_requirement":">= 0.1.0 <1.0.0"}
