{
  "name": "puppet-routing",
  "version": "0.3.1",
  "author": "john.bond@icann.org",
  "summary": "meta package used as an unified interface to puppet-quagga and puppet-openbgpd",
  "license": "Apache-2.0",
  "source": "https://github.com/icann-dns/puppet-routing",
  "project_page": "https://github.com/icann-dns/puppet-routing",
