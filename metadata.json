{
  "name": "puppet-routing",
  "version": "0.2.0",
  "author": "john.bond@icann.org",
  "summary": "meta package used as an unified interface to puppet-quagga and puppet-openbgpd",
  "license": "Apache-2.0",
  "source": "https://github.com/icann-dns/puppet-routing",
  "project_page": "https://github.com/icann-dns/puppet-routing",
  "issues_url": "https://github.com/icann-dns/puppet-routing/issues",
  "dependencies": [
    {"name":"puppetlabs-stdlib","version_requirement":">= 4.12.0 <5.0.0"},
    {"name":"icann-quagga","version_requirement":">= 0.4.1 <0.5.0"},
    {"name":"icann-openbgpd","version_requirement":">= 0.2.0 <0.3.0"},
    {"name":"icann-tea","version_requirement":">= 0.2.2 <0.3.0"}
  ],
  "operatingsystem_support": [
    {
      "operatingsystem" : "Ubuntu",
      "operatingsystemrelease" : [ "14.0.4", "16.04" ]
    },
    {
      "operatingsystem" : "FreeBSD",
      "operatingsystemrelease" : [ "10" ]
    }
  ]
}
